{
  "presets": [
    "@babel/env",
    "@babel/react",
    "@emotion/babel-preset-css-prop"
  ],
  "plugins": [
    "@babel/plugin-proposal-class-properties"
  ],
  "env": {
    "production": {
      "plugins": ["emotion"]
    },
    "development": {
      "plugins": [
        ["emotion", { "sourceMap": true }],
        "react-hot-loader/babel"
      ]
    }
  }
}
