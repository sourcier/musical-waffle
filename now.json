{
    "version": 2,
    "name": "musical-waffle",
    "alias": "sourcier.uk",
    "env": {
        "REACT_APP_PARSE_URL": "@musical-waffle-parse-url",
        "REACT_APP_PARSE_ID": "@musical-waffle-parse-id",
        "REACT_APP_PARSE_KEY": "@musical-waffle-parse-key"
    },
    "regions": [
        "lhr1"
    ],
    "builds": [
        {
            "src": "build/**/*",
            "use": "@now/static"
        }
    ],
    "routes": [
        {
            "src": "^/(.*)",
            "dest": "/build/$1"
        },
        {
            "handle": "filesystem"
        },
        {
            "src": "^/(.*)",
            "status": 404,
            "dest": "/build/404.html"
        }
    ],
    "github": {
        "enabled": false
    }
}
